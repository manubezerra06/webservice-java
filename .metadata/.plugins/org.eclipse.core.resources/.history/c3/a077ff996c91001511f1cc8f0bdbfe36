package br.edu.qi.loader;

import java.io.File;

import javax.xml.bind.JAXBContext;
import javax.xml.bind.JAXBElement;
import javax.xml.bind.JAXBException;
import javax.xml.bind.Unmarshaller;

import br.edu.qi.loader.dto.XmlModel;

public class Main {

	public static void main(String[] args) {
	
		JAXBContext context;
		try {
			
			context = JAXBContext.newInstance(XmlModel.class);
			Unmarshaller unmarshaller = context.createUnmarshaller();
			String path = "src/loadtest01.xml";
			File file  = new File(path);			
			Object unmarshallerResult = unmarshaller.unmarshal(file);
			XmlModel model = (XmlModel) unmarshallerResult;
									
			path = "src/loadtest02.xml";
			file  = new File(path);			
			unmarshallerResult = unmarshaller.unmarshal(file);
			model = (XmlModel) unmarshallerResult;
			
			
			path = "src/loadtest03.xml";
			file  = new File(path);			
			unmarshallerResult = unmarshaller.unmarshal(file);
			model = (XmlModel) unmarshallerResult;
			
			
			path = "src/loadtest04.xml";
			file  = new File(path);			
			unmarshallerResult = unmarshaller.unmarshal(file);
			model = (XmlModel) unmarshallerResult;
			
			
		} catch (JAXBException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		
	}
}
